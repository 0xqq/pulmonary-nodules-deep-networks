caffe-master/tools/extra/parse_log.sh  
caffe-master/tools/extra/extract_seconds.py
caffe-master/tools/extra/plot_training_log.py.example


# step1:保存日志文件
TOOLS=./build/tools
SOLVERFILE=./models/pulmonary_nodules_net_caffenet/solver.prototxt
$TOOLS/caffe train --solver=$SOLVERFILE 2>&1 |tee /root/code/Log-output/train_caffenet_ResNet50.log

TOOLS=./build/tools  
GLOG_logtostderr=0 GLOG_log_dir=/home/ucla/Downloads/Log-output/ \  
$TOOLS/caffe train \  
    --solver=models/pulmonary_nodules_net_caffenet/solver.prototxt  

# step2:解析日志文件
cp caffe-master/tools/extra/parse_log.sh /home/ucla/Downloads/Log-output/
cp caffe-master/tools/extra/extract_seconds.py /home/ucla/Downloads/Log-output/
cp caffe-master/tools/extra/plot_training_log.py.example /home/ucla/Downloads/Log-output/

./tools/extra/python parse_log.py train_caffenet_ResNet50.log ./
./parse_log.sh train_caffenet_ResNet50.log

# step3:绘制accuracy和loss曲线
python plot_training_log.py 2 testloss.png train_caffenet_ResNet50.log
./plot_training_log.py.example 0 save.png train_caffenet_ResNet50.log


python draw_loss.py --log train_caffenet_ResNet50.log
